    <RadialGradientBrush x:Key="FireBrush">
        <GradientStop Color="#FFFFFF" Offset="0.00"/>
        <GradientStop Color="#F8B51B" Offset="0.85"/>
        <GradientStop Color="#AD2B08" Offset="1.25"/>
    </RadialGradientBrush>
</ResourceDictionary>